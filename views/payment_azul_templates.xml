            <input type="hidden" id="CustomField2Value" name="CustomField2Value" t-att-value="Azul_CustomField2Value" />
            <!-- Auth -->
            <input type="hidden" id="AuthHash" name="AuthHash" t-att-value="Azul_AuthHash" />
        </template>
    </data>
</odoo>